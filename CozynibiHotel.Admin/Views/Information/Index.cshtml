@{
	ViewData["Title"] = "Custommer List";

}

@await Component.InvokeAsync("Loading")
<div class="content main-content" style="display:none;">
	<div class="intro-y flex items-center mt-8">
		<h2 class="text-lg font-medium mr-auto">
			Profile Layout
		</h2>
	</div>
	<!-- BEGIN: Profile Info -->
	<div class="intro-y box px-5 pt-5 mt-5">
		<div class="flex flex-col lg:flex-row border-b border-slate-200/60 dark:border-darkmode-400 pb-5 -mx-5">
			<div class="flex flex-1 px-5 items-center justify-center lg:justify-start">
				<div class="w-20 h-20 sm:w-24 sm:h-24 flex-none lg:w-32 lg:h-32 image-fit relative">
					<img id="inforLogo" alt="avt" class="rounded-full" src="">
					<div class="absolute mb-1 mr-1 flex items-center justify-center bottom-0 right-0 bg-primary rounded-full p-2"> <i class="w-4 h-4 text-white" data-lucide="camera"></i> </div>
				</div>
				<div class="ml-5">
					<div id="inforName" class="w-24 sm:w-40 truncate sm:whitespace-normal font-medium text-lg">Cozinibi Hotel</div>
					<div id="inforSlogon" class="text-slate-500">Brilliant choice for you</div>
				</div>
			</div>
			<div class="mt-6 lg:mt-0 flex-1 px-5 border-l border-r border-slate-200/60 dark:border-darkmode-400 border-t lg:border-t-0 pt-5 lg:pt-0">
				<div class="font-medium text-center lg:text-left lg:mt-3">Social Contact</div>
				<div class="flex flex-col justify-center items-center lg:items-start mt-4">
					<div class="truncate sm:whitespace-normal flex items-center"> <i data-lucide="facebook" class="w-4 h-4 mr-2"></i><span id="inforFacebook"></span> </div>
					<div class="truncate sm:whitespace-normal flex items-center mt-3"> <i data-lucide="instagram" class="w-4 h-4 mr-2"></i><span id="inforInstar"></span> </div>
					<div class="truncate sm:whitespace-normal flex items-center mt-3"> <i data-lucide="twitter" class="w-4 h-4 mr-2"></i><span id="inforTwitter"></span> </div>
					<div class="truncate sm:whitespace-normal flex items-center mt-3"> <i data-lucide="youtube" class="w-4 h-4 mr-2"></i><span id="inforYoutube"></span></div>
				</div>
			</div>
			<div class="mt-6 lg:mt-0 flex-1 px-5 border-l border-r border-slate-200/60 dark:border-darkmode-400 border-t lg:border-t-0 pt-5 lg:pt-0">
				<div class="font-medium text-center lg:text-left lg:mt-3">Directed Contact</div>
				<div class="flex flex-col justify-center items-center lg:items-start mt-4">
					<div class="truncate sm:whitespace-normal flex items-center"> <i data-lucide="map-pin" class="w-4 h-4 mr-2"></i><span id="inforAddress"></span> </div>
					<div class="truncate sm:whitespace-normal flex items-center mt-3"> <i data-lucide="phone" class="w-4 h-4 mr-2"></i> <span id="inforPhone"></span></div>
					<div class="truncate sm:whitespace-normal flex items-center mt-3"> <i data-lucide="mail" class="w-4 h-4 mr-2"></i> <span id="inforMail"></span></div>
					<div class="truncate sm:whitespace-normal flex items-center mt-3"> <i data-lucide="globe" class="w-4 h-4 mr-2"></i><span id="inforWeb"></span></div>
				</div>
			</div>
		</div>
		<ul class="nav nav-link-tabs flex-col sm:flex-row justify-center lg:justify-start text-center" role="tablist">
			<li id="dashboard-tab" class="nav-item" role="presentation"> <a href="javascript:;" class="nav-link py-4 active" data-tw-target="#dashboard" aria-controls="dashboard" aria-selected="true" role="tab">Details information </a> </li>
			<li id="partner-tab" class="nav-item" role="presentation"> <a href="javascript:;" class="nav-link py-4" data-tw-target="#partner" aria-selected="false" role="tab"> Our Partner </a> </li>
		</ul>
	</div>
	<!-- END: Profile Info -->
	<div class="intro-y tab-content mt-5">
		<div id="dashboard" class="tab-pane active" role="tabpanel" aria-labelledby="dashboard-tab">
			<div class="grid grid-cols-12 gap-5">
				<!-- BEGIN: Social Media -->
				<div class="intro-y box col-span-12 lg:col-span-6">
					<div class="flex items-center p-5 border-b border-slate-200/60 dark:border-darkmode-400">
						<h2 class="font-medium text-base mr-auto">
							Social Media Link
						</h2>
					</div>
					<div class="p-5">
						<div class="col-span-12 xl:col-span-6">
							<div>
								<label for="facebook" class="form-label flex"><i data-lucide="link" class="w-4 h-4 mr-2"></i> Facebook</label>
								<input id="facebook" type="text" class="form-control" value="russellcrowe@left4code.com" disabled>
							</div>
							<div class="mt-3">
								<label for="instagram" class="form-label flex"><i data-lucide="link" class="w-4 h-4 mr-2"></i>Instagram</label>
								<input id="instagram" type="text" class="form-control" value="Russell Crowe" disabled>
							</div>
							<div class="mt-3">
								<label for="twitter" class="form-label flex"><i data-lucide="link" class="w-4 h-4 mr-2"></i>Twitter</label>
								<input id="twitter" type="text" class="form-control" value="Russell Crowe" disabled>
							</div>
							<div class="mt-3">
								<label for="youtube" class="form-label flex"><i data-lucide="link" class="w-4 h-4 mr-2"></i>Youtube</label>
								<input id="youtube" type="text" class="form-control" value="Russell Crowe" disabled>
							</div>
						</div>

					</div>
				</div>
				<!-- END: Social Media -->
				<!-- BEGIN: Contact & address -->
				<div class="intro-y box col-span-12 lg:col-span-6">
					<div class="flex items-center p-5 border-b border-slate-200/60 dark:border-darkmode-400">
						<h2 class="font-medium text-base mr-auto">
							Contact & Address
						</h2>
					</div>
					<div class="p-5">
						<div class="col-span-12 xl:col-span-6">
							<div>
								<label for="phoneNumber" class="form-label"> Phone Number</label>
								<input id="phoneNumber" type="text" class="form-control" value="09223322" disabled>
							</div>
							<div class="mt-3">
								<label for="email" class="form-label">Email</label>
								<input id="email" type="text" class="form-control" value="RussellCrowe @@gmail.com" disabled>
							</div>
							<div class="mt-3">
								<label for="website" class="form-label">Website</label>
								<input id="website" type="text" class="form-control" value="ConizibiHotel.com" disabled>
							</div>
							<div class="mt-3">
								<label for="address" class="form-label">Address</label>
								<textarea id="address" class="form-control" disabled>10 Anson Road, International Plaza, #10-11, 079903 Singapore, Singapore</textarea>
							</div>
							<div class="mt-3">
								<label for="googleMap" class="form-label flex"><i data-lucide="link" class="w-4 h-4 mr-2"></i>Google Map Link</label>
								<input id="googleMap" type="text" class="form-control" value="https://www.google.com/maps/embed?pb=!1m23!1m12!1m3!1d20767.523870929625!2d105.80116097212839!3d21.008135515573954!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!4m8!3e6!4m0!4m5!1s0x3135ab7aba3cad95%3A0x7abbb4b806e8073d!2zVmnhu4duIEPDtG5nIE5naOG7hyBWw6AgxJDDoG8gVOG6oW8gRGV2bWFzdGVyLCBQaOG7kSBWxakgTmfhu41jIFBoYW4sIEzDoW5nIEjhuqEsIMSQ4buRbmcgxJBhLCBIw6AgTuG7mWk!3m2!1d21.013478199999998!2d105.8113758!5e0!3m2!1svi!2s!4v1672223894146!5m2!1svi!2s" disabled>
							</div>
							<div class="mt-3">
								<iframe title='gg-map'
										src=""
										style="border: 0;width:100%; height: 10rem"
										allowFullScreen=""
										loading="lazy"
										referrerPolicy="no-referrer-when-downgrade"
										class='gg-map'>
								</iframe>
							</div>
						</div>

					</div>
				</div>
				<!-- END: Contact & address -->
			</div>
		</div>
	</div>
</div>

@section Scripts{
<script>
	$('.side-information').addClass("side-menu--active");
	$('.side-information-list').addClass("side-menu--active");
	$('.side-information-wrapper').addClass("side-menu__sub-open");
</script>
<script src="~/js/information/information-index.js" type="module"></script>
}